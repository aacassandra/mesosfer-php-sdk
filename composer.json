{
    "name": "aacassandra/mesosfer-php-sdk",
    "description": "Mesosfer PHP SDK",
    "license": "BSD-3-Clause",
    "type": "library",
    "homepage": "https://github.com/aacassandra/mesosfer-php-sdk",
    "authors": [
        {
            "name": "aacassandra",
            "email": "alauddinafifcassandra@programmer.net"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.0.1",
        "phpdocumentor/phpdocumentor": "~2.9",
        "jms/serializer": "1.7.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Parse\\": "src/Parse/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parse\\Test\\": "tests/Parse/"
        }
    }
}
